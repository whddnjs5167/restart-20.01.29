#include <stdio.h>

int a[4];

int main()
{
    int tmp;
    
    scanf("%d %d %d", &a[0], &a[1], &a[2]);
    
    if(a[2]>a[1])
    {
        tmp=a[1];
        a[1]=a[2];
        a[2]=tmp;
    }
    if(a[1]>a[0])
    {
        tmp=a[0];
        a[0]=a[1];
        a[1]=tmp;
    }
    if(a[2]>a[1])
    {
        tmp=a[1];
        a[1]=a[2];
        a[2]=tmp;
    }
    
    printf("%d" ,a[1]);

    return 0;
}
